     declaration.`id_declaration` AS declaration_id_declaration,
     declaration.`id_membre` AS declaration_id_membre,
     declaration.`id_animal` AS declaration_id_animal,
     declaration.`lieu_declaration` AS declaration_lieu_declaration,
     declaration.`etat` AS declaration_etat,
     declaration.`commentaire` AS declaration_commentaire,
     declaration.`type` AS declaration_type,
     animal.`id_animal` AS animal_id_animal,
     animal.`espece` AS animal_espece,
     animal.`couleur` AS animal_couleur,
     animal.`type` AS animal_type,
     animal.`taille` AS animal_taille,
     animal.`age` AS animal_age,
